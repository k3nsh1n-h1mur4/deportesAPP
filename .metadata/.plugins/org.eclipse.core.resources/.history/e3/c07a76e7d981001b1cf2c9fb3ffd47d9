import java.awt.*;
import javax.swing.JButton;
import javax.swing.JFrame;




public class example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		JFrame jf = new JFrame("LogIn");
		JButton b = new JButton();
		b.setText("Apretar");
		
		jf.add(b);
		
		
		jf.setSize(400, 400);
		jf.setLayout(null);
		jf.setVisible(true);
		

	}

}
